{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT a.id, a.name, a.version, a.checksum_sha256,\n               am.metadata as \"metadata?\"\n        FROM artifacts a\n        LEFT JOIN artifact_metadata am ON am.artifact_id = a.id\n        WHERE a.repository_id = $1\n          AND a.is_deleted = false\n          AND a.name = $2\n        ORDER BY a.created_at ASC\n        ",
