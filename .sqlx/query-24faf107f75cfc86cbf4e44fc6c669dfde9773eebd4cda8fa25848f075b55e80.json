{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n                h.id, h.plugin_id, h.hook_type, h.handler_name, h.priority, h.is_enabled, h.created_at\n            FROM plugin_hooks h\n            INNER JOIN plugins p ON p.id = h.plugin_id\n            WHERE p.status = 'active' AND h.is_enabled = true\n            ORDER BY h.priority ASC\n            ",
