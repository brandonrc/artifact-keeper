  "query": "\n        SELECT\n            a.id,\n            a.path,\n            a.size_bytes,\n            a.created_at\n        FROM artifacts a\n        WHERE a.repository_id = $1\n          AND a.is_deleted = false\n          AND ($2 = '' OR a.path LIKE $2 || '%')\n        ORDER BY a.path\n        ",
